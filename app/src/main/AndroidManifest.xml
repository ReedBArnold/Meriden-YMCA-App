<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.arnold.reed.meridenymcaapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignInActivity"
            android:label="Sign In" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".RegistrationActivity"
            android:label="Registration" />
        <activity android:name=".AttendanceActivity" />
        <activity android:name=".GroupsActivity" />
        <activity android:name=".GroupDetailActivity"/>
        <activity android:name=".EmergenciesActivity" />
        <activity android:name=".ActivitiesActivity" />
        <activity android:name=".NewActivityScreen" />
        <activity android:name=".NewEmergencyActivity" />
        <activity android:name=".NewGroupActivity"></activity>
    </application>

</manifest>
